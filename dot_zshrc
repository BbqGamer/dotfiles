export ZSH_COMPDUMP="$HOME/.cache/zsh/zcompdump"
export PATH=/usr/local/bin:$PATH
export EDITOR='vim'

export ZSH="$HOME/.oh-my-zsh"
ZSH_THEME="robbyrussell"

plugins=(git)

source $ZSH/oh-my-zsh.sh
#
alias open=xdg-open
alias pman='f() { python3 -c "help($1)" };f'
alias calc='speedcrunch'
alias cls='clear'
alias lf='lfrun'

